{
  "name": "images",
  "version": "3.0.1",
  "description": "Cross-platform image decoder(webp/png/jpeg/gif) and encoder(webp/png/jpeg) for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "cd test && (node index.js; cd -)",
    "install": "node ./scripts/install.js",
    "configure": "cd gyp; rm -rf ./third-party/* && ./third-party.sh; cd ..; node-gyp configure",
    "build": "node-gyp build",
    "clean": "node-gyp clean",
    "rebuild": "node-gyp rebuild",
    "dist": "node dist.js"
  },
  "repositoryName": "zhangyuanwei/node-images",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangyuanwei/node-images.git"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "nan": "^2.3.2",
    "node-gyp": "^3.3.1",
    "npmlog": "^4.0.0",
    "request": "^2.79.0"
  },
  "keywords": [
    "image",
    "decoder",
    "encoder",
    "webp",
    "png",
    "jpeg",
    "gif",
    "cross-platform",
    "images"
  ],
  "author": "ZhangYuanwei <zhangyuanwei1988@gmail.com>",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/zhangyuanwei/node-images/issues"
  }
}
